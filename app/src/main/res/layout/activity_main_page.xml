<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_main"
    tools:context=".MainPageActivity" >

    <ImageView
        android:id="@+id/calisanView"
        android:layout_width="168dp"
        android:layout_height="147dp"
        android:layout_marginStart="122dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="51dp"
        app:layout_constraintBottom_toTopOf="@+id/istatistikView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/iconfinder_tshirt_622393" />

    <ImageView
        android:id="@+id/istatistikView"
        android:layout_width="168dp"
        android:layout_height="147dp"
        android:layout_marginStart="122dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/toDoView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/iconfinder_thefreeforty_profile_sheet_1243699" />

    <ImageView
        android:id="@+id/toDoView"
        android:layout_width="168dp"
        android:layout_height="147dp"
        android:layout_marginStart="122dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="183dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/iconfinder_resume_2795761" />
</android.support.constraint.ConstraintLayout>